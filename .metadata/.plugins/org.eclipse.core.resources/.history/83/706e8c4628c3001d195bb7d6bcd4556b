package linkedlist;

import java.util.*;

public class NodeUse {

	public static void main(String[] args) {
		Node<Integer> arr = new Node<>(1);
		Node<Integer> ar2 = new Node<>(2);
		arr.next = ar2;

		Node<Integer> head = arr;
		while(head!=null) {
			System.out.println(head.data);
			System.out.println(head);
			head = head.next;
		}

	}

}
