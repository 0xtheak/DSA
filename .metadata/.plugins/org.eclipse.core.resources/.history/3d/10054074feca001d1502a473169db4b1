package stacks;

public class StackUse {

	public static void main(String[] args)  {

		try{
			StackUsingArray st = new StackUsingArray(20);
			st.push(10);
			st.pop();
			
		}catch(StackEmptyException se) {
			System.out.println("catch block has been executed");
		}catch(StackFullException sf) {
			System.out.prinln("stack full exception has been occured");
		}
		
		

	}

}
